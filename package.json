{
  "name": "job-extractor",
  "version": "1.0.0",
  "description": "A TypeScript CLI tool for extracting job information from web pages using AI",
  "main": "dist/index.js",
  "bin": {
    "job-extractor": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli.ts",
    "start": "node -r dotenv/config -r tsconfig-paths/register dist/index.js",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --watch --config jest.config.js",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "job",
    "extractor",
    "cli",
    "typescript",
    "ai",
    "scraping"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.55.0",
    "@browserbasehq/stagehand": "^2.4.1",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "openai": "^4.24.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
