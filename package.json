{
  "name": "job-extractor",
  "version": "1.0.0",
  "description": "Monorepo for AI-powered job search toolkit - extraction, scoring, resume creation, and interview preparation",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "build:core": "npm run build --workspace=@inkredabull/job-extractor-core",
    "dev": "ts-node packages/core/src/cli.ts",
    "test": "npm run test --workspaces",
    "test:watch": "npm run test:watch --workspace=@inkredabull/job-extractor-core",
    "lint": "npm run lint --workspaces",
    "clean": "npm run clean --workspaces && rm -rf dist",
    "unified-server": "npm run start --workspace=@inkredabull/job-extractor-unified-server",
    "unified-server:llm": "npm run start:llm --workspace=@inkredabull/job-extractor-unified-server",
    "evaluate": "npm run eval --workspace=@job-extractor/evaluation",
    "evaluate:check": "cd packages/evaluation/python && source venv/bin/activate && python -m job_extractor_eval.cli --check-setup",
    "setup-gmail": "node scripts/setup-gmail-auth.js",
    "archive:jobs": "node scripts/archive-jobs.js"
  },
  "keywords": [
    "monorepo",
    "job",
    "extractor",
    "cli",
    "typescript",
    "ai",
    "resume",
    "interview",
    "chrome-extension",
    "unified-server",
    "evaluation",
    "langsmith"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.55.0",
    "@browserbasehq/stagehand": "^2.4.1",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@types/pdf-parse": "^1.1.5",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "google-auth-library": "^10.2.1",
    "googleapis": "^156.0.0",
    "js-yaml": "^4.1.0",
    "openai": "^4.24.0",
    "pdf-parse": "^2.4.5",
    "puppeteer": "^21.11.0"
  },
  "optionalDependencies": {
    "@inkredabull/macos-reminder": "file:../macos-reminder"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.5",
    "@types/puppeteer": "^7.0.4",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
