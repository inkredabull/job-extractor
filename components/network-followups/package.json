{
  "name": "@inkredabull/career-catalyst-network-followups",
  "version": "1.0.0",
  "description": "LinkedIn Network Follow-up Manager - Monthly outreach workflow with Claude-powered message variants",
  "main": "dist/Code.js",
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "deploy": "npm run build && clasp push",
    "deploy:watch": "npm run build && clasp push --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist/*",
    "prebuild": "npm run clean",
    "postbuild": "cp appsscript.json dist/",
    "clasp:login": "clasp login",
    "clasp:create": "clasp create --type sheets --title 'LinkedIn Network Followups'",
    "clasp:open": "clasp open"
  },
  "keywords": [
    "google-apps-script",
    "linkedin",
    "networking",
    "outreach",
    "ai",
    "typescript"
  ],
  "author": "Anthony Bull",
  "license": "MIT",
  "devDependencies": {
    "@google/clasp": "^2.4.2",
    "@types/google-apps-script": "^1.0.83",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "gas-webpack-plugin": "^2.6.0",
    "prettier": "^3.2.5",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
